{
    "name": "luissobrinho/lform",
    "description": "A remarkably magical form and input maker tool for Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Eduardo Altino",
            "email": "ads.luis.sobrinho@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "doctrine/dbal": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "mockery/mockery": "^0.9.4",
        "orchestra/database": "^3.4",
        "orchestra/testbench": "^3.4",
        "mikey179/vfsstream": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Luissobrinho\\LForm\\": "src/"
        },
        "files": [
          "src/Helpers/form_maker.php",
          "src/Helpers/input_maker.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Luissobrinho\\LForm\\LFormProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
